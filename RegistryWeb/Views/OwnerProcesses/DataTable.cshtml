@model RegistryWeb.ViewModel.OwnerProcessesListVM;
<a class="btn btn-success" asp-controller="OwnerProcesses" asp-action="Create">Добавить</a>
<table class="table table-hover">
    <thead>
        <tr>
            <th>№ по реестру</th>
            <th>Тип собственника</th>
            <th>Адрес</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.OwnerProcesses)
        {
            <tr>
                <td>@item.IdProcess</td>
                <td>@item.IdOwnerTypeNavigation.OwnerType1</td>
                <td>++++</td>
                <td>
                    <a class="btn btn-default oi oi-eye" title="Подробнее" aria-label="Подробнее"
                       asp-action="Details" asp-controller="OwnerProcesses" asp-route-idProcess="@item.IdProcess"></a>
                    <a class="btn btn-default oi oi-pencil" title="Изменить" aria-label="Изменить"
                       asp-action="Edit" asp-controller="OwnerProcesses" asp-route-idProcess="@item.IdProcess"></a>
                    <a class="btn btn-danger oi oi-x" title="Удалить" aria-label="Удалить"
                       asp-action="Delete" asp-controller="OwnerProcesses" asp-route-idProcess="@item.IdProcess"></a>
                </td>
            </tr>
        }
    </tbody>
</table>