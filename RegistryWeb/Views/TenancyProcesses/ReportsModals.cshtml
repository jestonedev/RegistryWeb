<form id="tenancyReasonForm">
    <div class="modal fade" id="tenancyReasonModal" tabindex="-1" role="dialog">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Добавить основание найма</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="form-row">
                        <div class="form-group col-12">
                            <label for="TenancyReason.ReasonNumber">Номер основания</label>
                            <input type="text" class="form-control" title="Номер основания" name="TenancyReason.ReasonNumber"
                                   data-val="true" data-val-required="Укажите номер основания" id="TenancyReason_ReasonNumber" />
                            <span class="text-danger field-validation-valid" data-valmsg-for="TenancyReason.ReasonNumber" data-valmsg-replace="true"></span>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-12">
                            <label for="TenancyReason.ReasonDate">Дата основания</label>
                            <input type="date" class="form-control" title="Дата основания" name="TenancyReason.ReasonDate"
                                   data-val="true" data-val-required="Укажите дату основания" id="TenancyReason_ReasonDate" />
                            <span class="text-danger field-validation-valid" data-valmsg-for="TenancyReason.ReasonDate" data-valmsg-replace="true"></span>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-12">
                            <label for="TenancyReason.IdReasonType">Тип основания</label>
                            <select class="selectpicker form-control" title="Тип основания" name="TenancyReason.IdReasonType"
                                    data-val="true" data-val-required="Выберите тип основания" id="TenancyReason_IdReasonType" asp-items="@reasonTypeList"></select>
                            <span class="text-danger field-validation-valid" data-valmsg-for="TenancyReason.IdReasonType" data-valmsg-replace="true"></span>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-check col-12">
                            <label>
                                <input type="checkbox" name="TenancyReason.IsDeletePrevReasons" />
                                Удалить существующие основания
                            </label>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary rr-report-submit">Добавить</button>
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Отмена</button>
                </div>
            </div>
        </div>
    </div>
</form>

<form id="noticeToBksForm">
    <div class="modal fade" id="noticeToBksModal" tabindex="-1" role="dialog">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Извещение в БКС</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <input type="hidden" name="NoticeToBks.IdProcess" />
                    <div class="form-row">
                        <div class="form-group col-12">
                            <label for="NoticeToBks.ActionText">Действие</label>
                            <select class="selectpicker form-control" title="Действие" name="NoticeToBks.ActionText"
                                    data-val="true" data-val-required="Поле обязательно для заполнения" id="NoticeToBks.ActionText">
                                <option value="заключен договор">заключен договор</option>
                                <option value="оформлено соглашение к договору">оформлено соглашение к договору</option>
                            </select>
                            <span class="text-danger field-validation-valid" data-valmsg-for="NoticeToBks.ActionText" data-valmsg-replace="true"></span>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-12">
                            <label for="NoticeToBks.PaymentType">Размер платы</label>
                            <select class="selectpicker form-control" title="Размер платы" name="NoticeToBks.PaymentType"
                                    data-val="true" data-val-required="Поле обязательно для заполнения" id="NoticeToBks.PaymentType">
                                <option value="0">По дате регистрации договора</option>
                                <option value="1">До 28.08.2019</option>
                                <option value="2">После 28.08.2019</option>
                            </select>
                            <span class="text-danger field-validation-valid" data-valmsg-for="NoticeToBks.PaymentType" data-valmsg-replace="true"></span>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-12">
                            <label for="NoticeToBks.Signer">Подписывающий</label>
                            <select class="selectpicker form-control" title="Подписывающий" name="NoticeToBks.Signer"
                                    data-val="true" data-val-required="Подписывающий обязателен для заполнения" id="NoticeToBks.Signer" asp-items="@bksSignersList"></select>
                            <span class="text-danger field-validation-valid" data-valmsg-for="NoticeToBks.Signer" data-valmsg-replace="true"></span>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary rr-report-submit">Сформировать</button>
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Отмена</button>
                </div>
            </div>
        </div>
    </div>
</form>